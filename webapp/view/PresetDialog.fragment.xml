<core:FragmentDefinition
  xmlns="sap.m"
  xmlns:core="sap.ui.core">
  <Dialog
    id="presetDialog"
    title="{i18nPresetDialog>titleText}"
    beforeOpen=".onBeforeOpen"
    afterClose=".onAfterClose">
    <beginButton>
      <Button
        text="{i18nPresetDialog>dialogCancelButtonText}"
        type="Reject"
        press=".onCloseDialog"/>
    </beginButton>
    <content>
      <List
        id="presetList"
        items="{presets>/}">
        <items>
          <ObjectListItem
            title="{presets>name}"
            type="Active"
            press=".onItemPress" />
        </items>
      </List>
    </content>
  </Dialog>
</core:FragmentDefinition>